{
  "rsa_encryption": {
    "key_size": 4096,
    "public_exponent": 65537,
    "hash_algorithm": "SHA256",
    "mgf_algorithm": "MGF1",
    "aes_key_size": 32,
    "aes_iv_size": 16,
    "aes_mode": "CBC",
    "key_directory": "rsa",
    "private_key_file": "rsa",
    "public_key_file": "rsa.pub",
    "private_key_permissions": "0o600",
    "public_key_permissions": "0o644"
  },
  "base64_encoding": {
    "validate": true,
    "auto_fix_padding": true
  },
  "key_generation": {
    "secure_random": true,
    "backend": "default"
  },
  "interactive_mode": {
    "clipboard_refresh": {
      "auto_detection_interval": 1.0,
      "max_content_size": 1048576,
      "enable_auto_detection_by_default": true,
      "show_character_count": true,
      "show_timestamps": true
    }
  },
  "logging": {
    "level": "INFO",
    "console_level": "INFO",
    "file_level": "DEBUG",
    "log_directory": "logs",
    "log_file": "string_multitool.log",
    "max_file_size": 10485760,
    "backup_count": 5,
    "format_console": "%(message)s",
    "format_file": "%(asctime)s - %(name)s - %(levelname)s - %(message)s",
    "date_format": "%Y-%m-%d %H:%M:%S",
    "additional_files": []
  }
}